<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>LoadDB</servlet-name>
        <servlet-class>Controllers.LoadDB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterStudents</servlet-name>
        <servlet-class>FilterStudents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterStudent</servlet-name>
        <servlet-class>Controllers.FilterStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadDB_jsp</servlet-name>
        <servlet-class>Controllers.LoadDB_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DAO</servlet-name>
        <servlet-class>DAL.DAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DBContext</servlet-name>
        <servlet-class>DAL.DBContext</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadDB</servlet-name>
        <url-pattern>/LoadDB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterStudents</servlet-name>
        <url-pattern>/FilterStudents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterStudent</servlet-name>
        <url-pattern>/FilterStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadDB_jsp</servlet-name>
        <url-pattern>/LoadDB_jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DAO</servlet-name>
        <url-pattern>/DAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DBContext</servlet-name>
        <url-pattern>/DBContext</url-pattern>
    </servlet-mapping>
</web-app>
