<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>MultiFilter</servlet-name>
        <servlet-class>Controllers.MultiFilter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadDB</servlet-name>
        <servlet-class>Controllers.LoadDB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadDB_jsp</servlet-name>
        <servlet-class>Controllers.LoadDB_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controllers.NewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MultiFilter</servlet-name>
        <url-pattern>/MultiFilter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadDB</servlet-name>
        <url-pattern>/LoadDB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadDB_jsp</servlet-name>
        <url-pattern>/LoadDB_jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
</web-app>
